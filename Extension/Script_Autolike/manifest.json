{
  "manifest_version": 2,
  "name": "Script Autolike",
  "short_name": "Script Autolike",
  "description": "Autolike Facebook",
  "version": "11.1.0.955",
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["js/content.js"],
      "css" : [],
      "run_at" : "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["js/background.js"]
  }
}
